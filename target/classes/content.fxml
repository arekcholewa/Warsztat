<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="945.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Interface">
   <children>
      <TableView fx:id="tableContent" layoutX="310.0" layoutY="29.0" prefHeight="526.0" prefWidth="616.0">
        <columns>
          <TableColumn fx:id="idView" prefWidth="75.0" text="C1" />
        </columns>
      </TableView>
      <CheckBox fx:id="chCars" layoutX="55.0" layoutY="115.0" mnemonicParsing="false" onAction="#isCarsSelected" prefHeight="25.0" prefWidth="167.0" text="Samochody" />
      <CheckBox fx:id="chParts" layoutX="55.0" layoutY="180.0" mnemonicParsing="false" onAction="#isPartsSelected" prefHeight="25.0" prefWidth="167.0" text="Części" />
      <CheckBox fx:id="chWarsztats" layoutX="55.0" layoutY="249.0" mnemonicParsing="false" onAction="#isWarsztatsSelected" prefHeight="25.0" prefWidth="167.0" selected="true" text="Warsztaty" />
      <Button fx:id="bDodaj" layoutX="29.0" layoutY="342.0" mnemonicParsing="false" onAction="#addToDatabase" text="Dodaj" />
      <Button fx:id="bUsun" layoutX="120.0" layoutY="342.0" mnemonicParsing="false" onAction="#deleteFromDatabase" text="Usuń" />
      <Button fx:id="bSzukaj" layoutX="222.0" layoutY="342.0" mnemonicParsing="false" onAction="#searchInDatabase" text="Szukaj" />
   </children>
</AnchorPane>
